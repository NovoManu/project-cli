{
  "name": "@novomanu/project-cli",
  "version": "0.0.2",
  "author": "Manu Ustenko",
  "description": "Manu Ustenko cli",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "mucli": "lib/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "format": "eslint src --fix --ext .ts && pretty-quick",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NovoManu/project-cli.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NovoManu/project-cli/issues"
  },
  "homepage": "https://github.com/NovoManu/project-cli#readme",
  "dependencies": {
    "@inquirer/confirm": "^0.0.19-alpha.0",
    "@inquirer/input": "^0.0.20-alpha.0",
    "@octokit/core": "^3.6.0",
    "chalk": "4.1.2",
    "commander": "^9.2.0",
    "dotenv": "^16.0.1",
    "nunjucks": "^3.2.3",
    "rimraf": "^3.0.2",
    "tar": "^6.1.11"
  },
  "devDependencies": {
    "@octokit/types": "^6.34.0",
    "@types/node": "^17.0.35",
    "@types/nunjucks": "^3.2.1",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "prettier-config-standard": "^5.0.0",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  }
}
